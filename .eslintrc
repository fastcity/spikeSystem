{
  "extends": "eslint-config-egg",
  "rules": {
    "indent": [
      "error",
      4
    ],
    "linebreak-style": [
      "error",
      "unix"
    ],
    "quotes": [
      "off",
      "single"
    ],
    "semi": [
      "off",
      "never"
    ],
    "array-bracket-spacing": [
      "error",
      "never"
    ]
  }
}
}